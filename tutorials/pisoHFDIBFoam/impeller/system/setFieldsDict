FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


regions
(

    sphereToCell
    {
        radius 1e-2;
        
        centre (0 0 0);

        fieldValues
        (
            volScalarFieldValue lambda 1
        );
    }
);

